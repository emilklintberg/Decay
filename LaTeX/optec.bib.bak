@MASTERSTHESIS{Geebelen2010,
  author = {K. Geebelen and J. Gillis},
  title = {Modelling and control of rotational start-up phase of tethered aeroplanes
	for wind energy harvesting},
  school = {{K}.{U}.{L}euven},
  year = {2010},
  month = {June},
  abstract = {'Modelling and control of rotational start-up phase of tethered aeroplanes
	for wind
	
	energy harvesting' looks at a novel approach at harvesting wind power,
	other than
	
	with wind turbines, 'crosswind kite power'. An aeroplane, anchored
	to the ground,
	
	transmits power to a ground-based generator by periodically pulling
	out the tether.
	
	In particular, this thesis focuses on a rotational start-up strategy
	of this system. A
	
	rotational start-up involves accelerating the aeroplane by towing
	it around a central
	
	axle. The tether is progressively enrolled until injection into an
	energy generating
	
	orbit at high altitude.
	
	On one hand, this thesis develops an indoor test platform to allow
	demonstrating
	
	rotational start-up on a small scale in the future. The test platform
	entails a measurement
	
	system, using stereovision, to estimate the position and orientation
	of the
	
	aeroplane. A first experiment with a radio-controlled aeroplane equipped
	with LEDs
	
	for clear marker visibility, shows the possibility for accurate state
	estimation. Indoor
	
	usage of the test platform excludes unpredictable disturbances of
	the wind.
	
	On the other hand, this thesis develops mathematical models to describe
	the dynamics
	
	of the system. The models allow simulating the tethered aeroplane
	and to find periodic
	
	orbits that are optimal in certain criteria. Model predictive control
	is used to follow
	
	these trajectories in simulation. Robustness of the system with respect
	to sidewind
	
	disturbance was assessed.},
  file = {Geebelen2010.pdf:Geebelen2010.pdf:PDF},
  keywords = {kitepower kite power planepower plane power carousel pumping cycle
	reversed optec highwind},
  owner = {KurtGeebelen},
  timestamp = {2010.05.31}
}

