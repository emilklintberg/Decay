@MASTERSTHESIS{Goos2011,
  author = {J. Goos},
  title = {{R}eversed {P}umping for {T}ethered
	
	{A}irplanes in {N}o-{W}ind {C}onditions},
  school = {K.U.Leuven},
  year = {2011},
  abstract = {In this master thesis, Reversed Pumping for Tethered Airplanes in
	No-Wind Conditions,
	
	we search for control strategies for a novel method to harvest wind
	power.
	
	Generally, wind is stronger and more consistent at higher altitudes.
	To reach these
	
	more interesting regions to exploit the wind speed, traditional wind
	turbines, require
	
	increasingly large expensive constructions. In the tethered kite power
	setup proposed
	
	by Loyd, energy is produced by periodically unrolling and retracting
	the cable
	
	of a tethered airplane, which is connected to a base station through
	a winch. This is
	
	called a pumping cycle.
	
	The main goal of this thesis was to find periodic orbits in absence
	of wind for
	
	the kite setup of the K.U.Leuven OPTEC Kite Power group. This means
	the energy
	
	required to sustain flight should be supplied purely by the base station!
	The trajectories
	
	are then optimized with respect to total power consumption.
	
	For small tether lengths, the plane can be supplied with power by
	means of a
	
	rotating carousel, resulting in a passively stable orbit. For a cable
	length longer than
	
	9 [m], the orbit cannot be sustained any longer by the carousel alone,
	and the winch
	
	starts to take over. Parallel to this thesis a winching system was
	designed for the
	
	Kite Power test setup, so the tether length could be varied. By applying
	a periodic
	
	energy consuming scheme called reversed pumping, it was theorized
	the airplane
	
	could be kept in the air even without wind. The found trajectories
	support this idea.},
  file = {Goos2011.pdf:Goos2011.pdf:PDF},
  owner = {jg},
  timestamp = {2012.05.14}
}

